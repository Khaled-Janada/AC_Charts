<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<SolidColorBrush x:Key="Foreground.Content.Dark" Color="#D8000000" />
	<SolidColorBrush x:Key="Foreground.Content.Darker" Color="#DD000000" />
	<SolidColorBrush x:Key="Foreground.Content.Light" Color="WhiteSmoke" />
	<SolidColorBrush x:Key="Foreground.Content.Lighter" Color="White" />

	<SolidColorBrush x:Key="Background.Light" Color="WhiteSmoke" />
	<SolidColorBrush x:Key="Background.Lighter" Color="#F8FCFCFC" />
</ResourceDictionary>
